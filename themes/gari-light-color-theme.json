	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#999",
				"fontStyle": "italic",
			}
		},
		{
			"scope": [
				"variable",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#333",
			}
		},
		{
			"scope": [
				"string",
				"constant.character",
			],
			"settings": {
				"foreground": "#080",
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
			],
			"settings": {
				"foreground": "#b0f",
			}
		},
		{
			"scope": [
				"entity",
				// for toml
				"support.type.property-name.array",
				"support.type.property-name.table",
			],
			"settings": {
				"foreground": "#f60",
			}
		},
		{
			"scope": [
				"constant",
				"support",
			],
			"settings": {
				"foreground": "#a00",
			}
		},
		{
			"scope": [
				"support.type.property-name",
				"variable.parameter.function-call",
			],
			"settings": {
				"foreground": "#04f",
			}
		},
		{
			"scope": [
				"variable.language",
				"variable.parameter.function.language.special",
				// wordlike operator for python 
				"keyword.operator.logical.python",
				// wordlike operator for c++
				"keyword.operator.new",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
			],
			"settings": {
				"foreground": "#00f",
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f00",
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#00f",
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#080"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#00f"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#080"
			}
		},
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"*.builtin": "#a00",
		"*.defaultLibrary": "#a00",
		"module": "#00f",
		"clsParameter": "#00f",
		"selfKeyword": "#00f",
		"selfParameter": "#00f",
		"parameter": "#04f",
		"tomlArrayKey": "#04f",
		"tomlTableKey": "#04f",
	},
}